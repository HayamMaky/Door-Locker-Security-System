#include"keypad.h"
#include"lcd.h"
/**********************************************************/
int main()
{
	char* str1 = "Plz enter Pass:";
	char ch ='*';
	char count = 0;
	LCD_init();


	while(1)
	{
		LCD_displayString(str1);
			for(count = 0; count < 5; count++)
			{
				KEYPAD_getPressedKey();
			    LCD_moveCursor(2,0);
				LCD_displayCharacter(ch);
			}
	}
}
